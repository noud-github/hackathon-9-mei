pipeline {
    agent { docker { image 'ubuntu' } }
    stages {
        stage('build') {
			steps {
				sh 'apt-get update && apt-get upgrade && apt-get install openssh-client --yes'
				sh 'pwd'
				sh 'ls -al config/jenkins/'
				sh 'ssh-agent -s'
				catchError {
					sshAgent ('Jenkins-CI') {
						sh 'ssh -o StrictHostKeyChecking=no -l pi 10.0.10.10 uname -a'
					}
                
            	}
				
				
				sh 'config/jenkins/test.cmd'
            }
        }
    }
}
